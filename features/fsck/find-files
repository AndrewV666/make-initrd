#!/bin/sh -efu

for m in $MOUNTPOINTS; do
	fstype="$(findmnt -n -o FSTYPE --target "$m")"
	which --skip-alias --skip-functions "fsck.$fstype"

	case "$fstype" in
		ext2|ext3|ext4*)
			conf="$SYSCONFDIR/e2fsck.conf"
			[ ! -f "$conf" ] || printf '%s\n' "$conf"
			;;
	esac
done
which --skip-alias --skip-functions fsck
